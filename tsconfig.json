{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"outDir": "./bin/js",
		"moduleResolution": "node",
		"strictPropertyInitialization": false
	},
	"include": ["src/**/*"]
}
